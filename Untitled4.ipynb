   "source": [
    "import pyttsx3\n",
    "from time import sleep\n",
    "\n",
    "b = open(\"demo.txt\", \"r\")\n",
    "book = b.readlines()\n",
    "engine = pyttsx3.init()\n",
    "\n",
    "for i in book:\n",
    "    engine.say(i)\n",
    "    engine.runAndWait()\n",
    "    sleep(2)\n",
    "\n",
    "b.close()  # This should be outside the loop\n"
   ]
